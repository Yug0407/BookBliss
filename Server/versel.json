  {
    "version": 2,
    "functions": {
        "api/clerk/webhook": {
            "memory": 1024,
            "maxDuration": 10,
            "excludeFiles": "node_modules/.prisma/**"
        }
    },
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}